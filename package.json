    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "autoprefixer": "10.4.7",
    "contentlayer": "0.2.4",
    "eslint": "8.20.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "8.5.0",
