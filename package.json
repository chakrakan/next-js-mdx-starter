{
  "name": "next-ts-mdx-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn meta && next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-all": "yarn check-format && yarn check-types && yarn build",
    "prepare": "husky install",
    "meta": "node scripts/meta.mjs"
  },
  "dependencies": {
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.2",
    "@types/node": "17.0.27",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "autoprefixer": "10.4.5",
    "contentlayer": "0.2.4",
    "eslint": "8.14.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "globby": "13.1.1",
    "husky": "7.0.4",
    "next-contentlayer": "0.2.4",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "reading-time": "1.5.0",
    "rehype-accessible-emojis": "0.3.2",
    "rehype-autolink-headings": "6.1.1",
    "rehype-code-titles": "1.0.3",
    "rehype-prism-plus": "1.3.2",
    "rehype-slug": "5.0.1",
    "remark-gfm": "3.0.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3"
  }
